{
    "name": "wame/tiptap",
    "description": "Laravel Nova 4 Tiptap editor field.",
    "keywords": [
        "laravel",
        "nova",
        "nova4",
        "tiptap",
        "vue",
        "editor"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Wame\\Tiptap\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wame\\Tiptap\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
